{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/ts/*"
      ],
      "@utils/*": [
        "src/ts/utils/*"
      ],
      "@libs/*": [
        "src/ts/libs/*"
      ],
      "@types/*": [
        "src/ts/types/*"
      ],
      "~/*": [
        "node_modules/*"
      ]
    },
  },
}